    "karma-coverage": "2.2.0",
    "karma-spec-reporter": "0.0.34",
    "karma-jasmine": "5.1.0",
    "@types/jasmine": "4.3.0",
    "cypress": "12.1.0",
    "@cypress/code-coverage": "3.10.0",
    "cypress-file-upload": "5.0.8",
    "cypress-fail-fast": "7.0.0",
    "istanbul-instrumenter-loader": "3.0.1",
    "nyc": "15.1.0",
    "start-server-and-test": "1.15.2",
    "node-xlsx": "0.21.0"
  },
  "browserslist": [
    "defaults",
    "cover 99%",
    "not safari < 11",
    "not ios_saf < 11",
    "not ie < 99",
    "not ie_mob < 99",
    "not firefox <= 57",
