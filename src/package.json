{
  "name": "rapydo",
  "version": "2.3.0",
  "private": true,
  "description": "Frontend for RAPyDo framework",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --port 8080 --watch --live-reload",
    "start:for-cypress": "ng serve --configuration cypress --host 0.0.0.0 --port 8080 --disable-host-check",
    "build": "ng build --configuration=production",
    "build:ssr": "ng run RAPyDo:server:production && ng run RAPyDo:prerender --routes /",
    "move-build-online": "rm -rf /app/dist_online && mv /app/dist /app/dist_online",
    "gzip": "gzipper compress --gzip-level 9 --threshold 512 --remove-larger --skip-compressed --exclude png,jpeg,jpg,svg,ico,woff,woff2,gif,webp /app/dist",
    "test": "ng test --code-coverage",
    "test:single": "ng test --code-coverage --watch=false",
    "cypress": "cypress run",
    "cypress:start:full": "start-server-and-test start:for-cypress http://localhost:8080 'cypress run --record --group full --browser chrome'",
    "cypress:start:totp": "start-server-and-test start:for-cypress http://localhost:8080 'cypress run --record --group totp --browser chrome'",
    "cypress:start:lite": "start-server-and-test start:for-cypress http://localhost:8080 'cypress run --record --group lite --browser chrome'",
    "cypress:start:hiddenlogin": "start-server-and-test start:for-cypress http://localhost:8080 'cypress run --record --group hiddenlogin --browser chrome'",
    "cypress:start:norecord": "start-server-and-test start:for-cypress http://localhost:8080 'cypress run --browser chrome'"
  },
  "license": "MIT",
  "dependencies": {
    "typescript": "4.6.3",
    "tslib": "2.3.1",
    "@angular/common": "13.3.0",
    "@angular/compiler": "13.3.0",
    "@angular/core": "13.3.0",
    "@angular/forms": "13.3.0",
    "@angular/platform-browser": "13.3.0",
    "@angular/platform-browser-dynamic": "13.3.0",
    "@angular/router": "13.3.0",
    "@angular/animations": "13.3.0",
    "@angular/localize": "13.3.0",
    "@angular/language-service": "13.3.0",
    "@angular/platform-server": "13.3.0",
    "@angular/compiler-cli": "13.3.0",
    "@angular/cli": "13.3.0",
    "@angular-devkit/build-angular": "13.3.0",
    "@nguniversal/builders": "13.1.0",
    "ngx-build-plus": "13.0.1",
    "core-js": "3.21.1",
    "zone.js": "0.11.4",
    "rxjs": "7.5.1",
    "bootstrap": "5.1.3",
    "@ng-bootstrap/ng-bootstrap": "12.0.1",
    "@popperjs/core": "2.11.2",
    "@fortawesome/fontawesome-free": "6.1.1",
    "@ngx-formly/core": "5.11.2",
    "@ngx-formly/bootstrap": "5.11.2",
    "@ng-select/ng-select": "8.1.1",
    "@ng-select/ng-option-highlight": "0.0.7",
    "@swimlane/ngx-datatable": "20.0.0",
    "moment": "2.29.1",
    "ngx-moment": "5.0.0",
    "ngx-spinner": "13.0.0",
    "ngx-clipboard": "15.0.1",
    "ngx-uploadx": "5.0.0",
    "file-saver-es": "2.0.5",
    "angular2-cookie-law": "7.0.1",
    "generate-password-browser": "1.1.0",
    "ngx-toastr": "14.2.1",
    "ngx-google-analytics": "13.0.1",
    "@sentry/browser": "6.19.2",
    "xlsx": "0.18.5",
    "ajv": "^8.0.0",
    "ajv-formats": "2.1.1",
    "angular-polyfills": "1.0.1",
    "localstorage-polyfill": "1.0.1",
    "gzipper": "7.0.0"
  },
  "devDependencies": {
    "karma": "6.3.17",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage": "2.2.0",
    "karma-spec-reporter": "0.0.33",
    "karma-jasmine": "4.0.1",
    "@types/jasmine": "3.10.3",
    "cypress": "9.5.2",
    "@cypress/code-coverage": "3.9.12",
    "cypress-file-upload": "5.0.8",
    "istanbul-instrumenter-loader": "3.0.1",
    "nyc": "15.1.0",
    "start-server-and-test": "1.14.0",
    "otpauth": "7.1.0",
    "node-xlsx": "0.21.0"
  },
  "browserslist": [
    "defaults",
    "cover 99%",
    "not safari < 9",
    "not ios_saf < 9",
    "not ie < 99",
    "not ie_mob < 99",
    "not firefox < 40",
    "not chrome < 40"
  ]
}
