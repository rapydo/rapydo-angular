{
  "name": "rapydo",
  "version": "1.1.0",
  "private": true,
  "description": "Frontend for RAPyDo framework",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --port 8080 --watch --live-reload",
    "start:for-cypress": "ng serve --configuration cypress --host 0.0.0.0 --port 8080 --disableHostCheck",
    "courtesy": "rimraf dist/* && copyfiles --flat /app/courtesy/index.html /app/dist",
    "build": "ng build --prod --optimization=true --buildOptimizer=true --extractLicenses=true --progress=true",
    "build:ssr": "ng run RAPyDo:server:production && ng run RAPyDo:prerender",
    "gzip": "gzipper compress --verbose --level 9 --threshold 512 --remove-larger --exclude png,jpeg,jpg,svg,ico,woff,woff2,gif,webp /app/dist",
    "test": "ng test --code-coverage",
    "test:single": "ng test --code-coverage --watch=false",
    "cypress": "cypress run",
    "cypress:start": "start-server-and-test start:for-cypress http://localhost:8080 'cypress run --record --group full --browser chrome'",
    "cypress:start:totp": "start-server-and-test start:for-cypress http://localhost:8080 'cypress run --record --group totp --browser chrome'",
    "cypress:start:lite": "start-server-and-test start:for-cypress http://localhost:8080 'cypress run --record --group lite --browser chrome'",
    "cypress:start:norecord": "start-server-and-test start:for-cypress http://localhost:8080 'cypress run --browser chrome'"
  },
  "license": "MIT",
  "dependencies": {
    "typescript": "4.1.5",
    "tslib": "2.1.0",
    "@angular/common": "11.2.3",
    "@angular/compiler": "11.2.3",
    "@angular/core": "11.2.3",
    "@angular/forms": "11.2.3",
    "@angular/platform-browser": "11.2.3",
    "@angular/platform-browser-dynamic": "11.2.3",
    "@angular/router": "11.2.3",
    "@angular/animations": "11.2.3",
    "@angular/localize": "11.2.3",
    "@angular/language-service": "11.2.3",
    "@angular/platform-server": "11.2.3",
    "@angular/compiler-cli": "11.2.3",
    "@angular/cli": "11.2.2",
    "@angular-devkit/build-angular": "0.1102.1",
    "@nguniversal/builders": "11.2.0",
    "@types/node": "14.14.31",
    "core-js": "3.9.1",
    "zone.js": "0.11.4",
    "rxjs": "6.6.6",
    "bootstrap": "4.6.0",
    "@ng-bootstrap/ng-bootstrap": "9.0.2",
    "@fortawesome/fontawesome-free": "5.15.2",
    "@ngx-formly/core": "5.10.14",
    "@ngx-formly/bootstrap": "5.10.14",
    "@ng-select/ng-select": "6.1.0",
    "@swimlane/ngx-datatable": "19.0.0",
    "moment": "2.29.1",
    "ngx-moment": "5.0.0",
    "ngx-spinner": "10.0.1",
    "ngx-clipboard": "14.0.1",
    "ngx-uploadx": "4.0.2",
    "file-saver": "2.0.5",
    "@types/file-saver": "2.0.1",
    "angular2-cookie-law": "7.0.1",
    "ngx-toastr": "13.2.0",
    "ngx-google-analytics": "11.2.0",
    "@sentry/browser": "6.2.0",
    "ngx-device-detector": "2.0.6",
    "xlsx": "0.16.9",
    "ajv": "^7.0.0",
    "ajv-formats": "1.5.1",
    "angular-polyfills": "1.0.1",
