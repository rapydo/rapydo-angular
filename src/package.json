{
  "name": "rapydo",
  "version": "1.0.0",
  "private": true,
  "description": "Frontend for RAPyDo framework",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --port 8080 --aot --watch --live-reload --progress",
    "start:for-cypress": "ng serve --configuration cypress --host 0.0.0.0 --port 8080 --aot --disableHostCheck",
    "courtesy": "rimraf dist/* && copyfiles --flat /app/courtesy/index.html /app/dist",
    "build": "ng build --prod --optimization=true --buildOptimizer=true --extractLicenses=true --progress=true",
    "gzip": "gzipper compress --verbose --level 9 --threshold 256 --remove-larger --exclude png,jpeg,jpg,svg,ico,woff,woff2,gif,webp /app/dist",
    "test": "ng test --code-coverage",
    "test:single": "ng test --code-coverage --watch=false",
    "cypress": "cypress run",
    "cypress:start": "start-server-and-test start:for-cypress http://localhost:8080 'cypress run --record --group full --browser chrome'",
    "cypress:start:lite": "start-server-and-test start:for-cypress http://localhost:8080 'cypress run --record --group lite --browser chrome'",
    "cypress:start:norecord": "start-server-and-test start:for-cypress http://localhost:8080 'cypress run --browser chrome'"
  },
  "license": "MIT",
  "dependencies": {
    "typescript": "4.0.5",
    "tslib": "2.0.3",
    "@angular/common": "11.0.4",
    "@angular/compiler": "11.0.4",
    "@angular/core": "11.0.4",
    "@angular/forms": "11.0.4",
    "@angular/platform-browser": "11.0.4",
    "@angular/platform-browser-dynamic": "11.0.4",
    "@angular/router": "11.0.4",
    "@angular/animations": "11.0.4",
    "@angular/localize": "11.0.4",
    "@angular/language-service": "11.0.4",
    "@angular/compiler-cli": "11.0.4",
    "@angular/cli": "11.0.4",
    "@angular-devkit/build-angular": "0.1100.4",
    "core-js": "3.6.5",
    "zone.js": "0.11.2",
    "rxjs": "6.6.3",
    "bootstrap": "4.5.3",
    "@ng-bootstrap/ng-bootstrap": "7.0.0",
    "ngx-clipboard": "14.0.1",
    "@ngx-formly/core": "5.10.6",
    "@ngx-formly/bootstrap": "5.10.6",
    "@swimlane/ngx-datatable": "18.0.0",
    "moment": "2.29.1",
    "ngx-moment": "5.0.0",
    "ngx-uploadx": "4.0.1",
    "file-saver": "2.0.5",
    "@types/file-saver": "2.0.1",
    "angular2-cookie-law": "7.0.1",
    "@fortawesome/fontawesome-free": "5.15.1",
    "ngx-toastr": "13.1.0",
    "ngx-google-analytics": "11.0.0",
    "@sentry/browser": "5.29.0",
    "ngx-device-detector": "2.0.4",
    "ngx-spinner": "10.0.1",
    "xlsx": "0.16.9",
    "ajv": "6.12.6",
    "angular-polyfills": "1.0.1",
    "gzipper": "4.3.0",
    "rimraf": "3.0.2",
    "copyfiles": "2.4.1"
  },
  "devDependencies": {
    "karma": "5.2.3",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-spec-reporter": "0.0.32",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "6.0.0",
    "@types/jasmine": "3.6.2",
    "@types/jasminewd2": "2.0.8",
    "cypress": "6.1.0",
    "cypress-localstorage-commands": "1.3.1",
    "@cypress/code-coverage": "3.8.7",
    "istanbul-instrumenter-loader": "3.0.1",
    "nyc": "15.1.0",
    "@istanbuljs/nyc-config-typescript": "1.0.1",
    "source-map-support": "0.5.19",
    "ngx-build-plus": "10.1.1",
    "start-server-and-test": "1.11.6",
    "raw-loader": "4.0.2"
  }
}
