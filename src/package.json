{
  "name": "rapydo",
  "version": "0.7.1",
  "private": true,
  "description": "Frontend for RAPyDo framework",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --port 8080 --poll 1000 --aot",
    "start-cypress": "ng serve --host 0.0.0.0 --port 8080 --aot --disable-host-check",
    "build": "rimraf dist/* && copyfiles --flat /app/courtesy/index.html /app/dist && ng build --deleteOutputPath=false --optimization=true --prod --extractCss=false && gzipper --verbose /app/dist",
    "test": "ng test --codeCoverage",
    "single-test": "ng test --codeCoverage --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "license": "MIT",
  "dependencies": {

    "@angular/common": "8.2.14",
    "@angular/compiler": "8.2.14",
    "@angular/core": "8.2.14",
    "@angular/forms": "8.2.14",
    "@angular/platform-browser": "8.2.14",
    "@angular/platform-browser-dynamic": "8.2.14",
    "@angular/router": "8.2.14",
    "@angular/animations": "8.2.14",

    "rxjs": "6.5.3",
    "rxjs-compat": "6.5.3",
    "tslib": "1.10.0",
    "zone.js": "0.10.2",

    "bootstrap": "4.4.1",
    "@ng-bootstrap/ng-bootstrap": "5.1.4",
    "ngx-clipboard": "12.3.0",
    "@ngx-formly/core": "5.5.6",
    "@ngx-formly/bootstrap": "5.5.6",
    "@swimlane/ngx-datatable": "16.0.2",
    "moment": "2.24.0",
    "ngx-moment": "3.5.0",
    "angular-confirmation-popover": "4.2.1",
    "ngx-uploadx": "3.3.2",
    "file-saver": "2.0.2",
    "@types/file-saver": "2.0.1",
    "angular2-cookie-law": "7.0.1",
    "@fortawesome/fontawesome-free": "5.12.0",
    "material-icons": "0.3.1",
    "ngx-toastr": "11.2.1",
    "ngx-google-analytics": "8.0.0",
    "@sentry/browser": "5.10.1",

    "core-js": "3.4.7",
    "classlist.js": "1.1.20150312"
  },
  "devDependencies": {
    "@angular/cli": "8.3.20",
    "typescript": "3.5.3",

    "@angular-devkit/build-angular": "0.803.20",
    "@angular/compiler-cli": "8.2.14",
    "@angular/language-service": "8.2.14",

    "@types/node": "12.12.14",
    "@types/jasmine": "3.5.0",
    "@types/jasminewd2": "2.0.8",

    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.4.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.1",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "karma-coveralls": "2.1.0",
    "protractor": "5.4.2",

    "codelyzer": "5.2.0",
    "ts-node": "8.5.4",
    "tslint": "5.20.1",
    "raw-loader": "4.0.0",

    "rimraf": "3.0.0",
