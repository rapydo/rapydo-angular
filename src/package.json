{
  "name": "rapydo",
  "version": "0.8.0",
  "private": true,
  "description": "Frontend for RAPyDo framework",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --port 8080 --aot --watch --live-reload --progress",
    "start:for-cypress": "ng serve --configuration serve-for-cypress --host 0.0.0.0 --port 8080 --aot --disableHostCheck",
    "courtesy": "rimraf dist/* && copyfiles --flat /app/courtesy/index.html /app/dist",
    "build": "ng build --prod --optimization=true --buildOptimizer=true --extractLicenses=true --progress=true",
    "gzip": "gzipper compress --verbose --level 9 /app/dist",
    "test": "ng test --code-coverage",
    "test:single": "ng test --code-coverage --watch=false",
    "cypress": "cypress run",
    "cypress:start:electron": "start-server-and-test start:for-cypress http://localhost:8080 'cypress run --record --group electron --browser electron'",
    "cypress:start:chrome": "start-server-and-test start:for-cypress http://localhost:8080 'cypress run --record --group chrome --browser chrome'",
    "cypress:start:chromium": "start-server-and-test start:for-cypress http://localhost:8080 'cypress run --record --group chromium --browser chromium'",
    "cypress:start:firefox": "start-server-and-test start:for-cypress http://localhost:8080 'cypress run --record --group firefox --browser firefox'"
  },
  "license": "MIT",
  "dependencies": {
    "typescript": "3.9.7",
    "tslib": "2.0.1",
    "@angular/common": "10.1.1",
    "@angular/compiler": "10.1.1",
    "@angular/core": "10.1.1",
    "@angular/forms": "10.1.1",
    "@angular/platform-browser": "10.1.1",
    "@angular/platform-browser-dynamic": "10.1.1",
    "@angular/router": "10.1.1",
    "@angular/animations": "10.1.1",
    "@angular/localize": "10.1.1",
    "@angular/language-service": "10.1.1",
    "@angular/compiler-cli": "10.1.1",
    "@angular/cli": "10.1.1",
    "@angular-devkit/build-angular": "0.1001.1",
    "core-js": "3.6.5",
    "zone.js": "0.11.1",
    "rxjs": "6.6.3",
    "bootstrap": "4.5.2",
    "@ng-bootstrap/ng-bootstrap": "7.0.0",
    "ngx-clipboard": "13.0.1",
    "@ngx-formly/core": "5.10.1",
    "@ngx-formly/bootstrap": "5.10.1",
    "@swimlane/ngx-datatable": "17.1.0",
    "moment": "2.28.0",
    "ngx-moment": "5.0.0",
    "angular-confirmation-popover": "6.0.0",
    "ngx-uploadx": "3.5.1",
    "file-saver": "2.0.2",
    "@types/file-saver": "2.0.1",
    "angular2-cookie-law": "7.0.1",
    "@fortawesome/fontawesome-free": "5.14.0",
    "ngx-toastr": "13.0.0",
    "ngx-google-analytics": "9.0.1",
    "@sentry/browser": "5.23.0",
    "ngx-device-detector": "2.0.0",
    "ngx-spinner": "10.0.1",
    "xlsx": "0.16.7",
    "ajv": "6.12.5",
    "angular-polyfills": "1.0.1",
    "gzipper": "4.0.2",
    "rimraf": "3.0.2",
    "copyfiles": "2.3.0"
  },
  "devDependencies": {
    "karma": "5.2.2",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-spec-reporter": "0.0.32",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "5.0.2",
    "@types/jasmine": "3.5.14",
    "@types/jasminewd2": "2.0.8",
    "cypress": "5.1.0",
    "cypress-localstorage-commands": "1.2.2",
    "@cypress/code-coverage": "3.8.1",
    "istanbul-instrumenter-loader": "3.0.1",
    "nyc": "15.1.0",
    "@istanbuljs/nyc-config-typescript": "1.0.1",
    "source-map-support": "0.5.19",
    "ngx-build-plus": "10.1.1",
    "start-server-and-test": "1.11.3",
    "raw-loader": "4.0.1"
  }
}
