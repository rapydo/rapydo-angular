{
  "name": "rapydo",
  "version": "3.1.0",
  "private": true,
  "description": "Frontend for RAPyDo framework",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --port 8080 --watch --live-reload",
    "start:for-cypress": "ng serve --configuration cypress --host 0.0.0.0 --port 8080 --disable-host-check",
    "build": "ng build --configuration=production",
    "build:ssr": "ng run RAPyDo:server:production && ng run RAPyDo:prerender --routes /",
    "move-build-online": "rm -rf /app/dist_online && mv /app/dist /app/dist_online",
    "gzip": "gzipper compress --gzip-level 9 --threshold 512 --remove-larger --skip-compressed --exclude png,jpeg,jpg,svg,ico,woff,woff2,gif,webp /app/dist",
    "test": "ng test --code-coverage",
    "test:single": "ng test --code-coverage --watch=false",
    "cypress": "cypress run",
    "cypress:start:full": "start-server-and-test start:for-cypress http://localhost:8080 'cypress run --record --group full --browser chrome'",
    "cypress:start:totp": "start-server-and-test start:for-cypress http://localhost:8080 'cypress run --record --group totp --browser chrome'",
    "cypress:start:lite": "start-server-and-test start:for-cypress http://localhost:8080 'cypress run --record --group lite --browser chrome'",
    "cypress:start:norecord": "start-server-and-test start:for-cypress http://localhost:8080 'cypress run --browser chrome'"
  },
  "license": "MIT",
  "dependencies": {
    "typescript": "4.9.5",
    "tslib": "2.6.2",
    "@angular/common": "17.3.6",
    "@angular/compiler": "17.3.6",
    "@angular/core": "17.3.6",
    "@angular/forms": "17.3.6",
    "@angular/platform-browser": "17.3.6",
    "@angular/platform-browser-dynamic": "17.3.6",
    "@angular/router": "17.3.6",
    "@angular/animations": "17.3.6",
    "@angular/localize": "17.3.6",
    "@angular/language-service": "17.3.6",
    "@angular/platform-server": "17.3.6",
    "@angular/compiler-cli": "17.3.6",
    "@angular/cli": "17.3.6",
    "@angular-devkit/architect": "0.1703.6",
    "@angular-devkit/build-angular": "17.3.6",
    "@angular-devkit/core": "17.3.6",
    "@nguniversal/builders": "15.2.1",
    "ngx-build-plus": "17.0.0",
    "zone.js": "0.13.1",
    "rxjs": "7.8.1",
    "bootstrap": "5.3.3",
    "@ng-bootstrap/ng-bootstrap": "16.0.0",
    "@popperjs/core": "2.11.8",
    "@fortawesome/fontawesome-free": "6.5.1",
    "@ngx-formly/core": "6.3.0",
    "@ngx-formly/bootstrap": "6.3.0",
    "@ng-select/ng-select": "12.0.7",
    "@ng-select/ng-option-highlight": "12.0.6",
    "@swimlane/ngx-datatable": "20.1.0",
    "date-fns": "2.30.0",
    "ngx-date-fns": "10.0.1",
    "ngx-spinner": "15.0.1",
    "ngx-clipboard": "16.0.0",
    "ngx-uploadx": "6.2.0",
    "file-saver-es": "2.0.5",
    "angular2-cookie-law": "7.0.1",
    "generate-password-browser": "1.1.0",
    "ngx-toastr": "18.0.0",
    "@sentry/browser": "7.106.0",
