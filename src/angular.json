{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "RAPyDo": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "./",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "/app/dist",
            "index": "app/rapydo/index.html",
            "main": "app/rapydo/main.ts",
            "deleteOutputPath": false,
            "polyfills": "polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "sourceMap": true,
            "extractLicenses": true,
            "buildOptimizer": false,
            "optimization": false,
            "progress": true,
            "assets": [
              "node_modules/@fortawesome/fontawesome-free/webfonts",
              "./app/rapydo/assets",
              "./app/custom/assets",
              "./app/subproject/assets"
            ],
            "styles": ["./app/rapydo/styles/rapydo.scss"],
            "scripts": [],
            "allowedCommonJsDependencies": ["xlsx", "@rapydo/../../validate.js"]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "app/rapydo/environments/environment.ts",
                  "with": "app/rapydo/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "namedChunks": false,
              "aot": true,
              "sourceMap": false,
              "buildOptimizer": true,
              "optimization": {
                "scripts": true,
                "styles": {
                  "minify": true,
                  "inlineCritical": false
                },
                "fonts": {
                  "inline": true
                }
              },
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "3mb",
                  "maximumError": "6mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "/app/dist-server",
            "main": "app/rapydo/main.server.ts",
            "tsConfig": "tsconfig.server.json"
          },
          "configurations": {
            "production": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "app/rapydo/environments/environment.ts",
                  "with": "app/rapydo/environments/environment.prod.ts"
                }
              ],
              "sourceMap": false,
              "optimization": true
            }
          }
        },
        "prerender": {
          "builder": "@nguniversal/builders:prerender",
          "options": {
            "browserTarget": "RAPyDo:build:production",
            "serverTarget": "RAPyDo:server:production",
            "routes": []
          }
        },
        "serve": {
          "builder": "ngx-build-plus:dev-server",
          "options": {
            "browserTarget": "RAPyDo:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "RAPyDo:build:production"
            },
            "cypress": {
              "extraWebpackConfig": "./cypress/coverage.webpack.js"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "RAPyDo:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "app/rapydo/tests.ts",
            "polyfills": "polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": ["./app/rapydo/assets", "./app/custom/assets"],
            "styles": ["./app/rapydo/styles/rapydo.scss"],
            "scripts": []
          }
        }
      }
    }
  },
  "defaultProject": "RAPyDo"
}
