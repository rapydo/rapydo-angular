{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "RAPyDo": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "./",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "/app/dist",
            "index": "app/rapydo/index.html",
            "main": "app/rapydo/main.ts",
            "polyfills": "polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": false,
            "assets": [
              "./app/rapydo/assets",
              "./app/custom/assets"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/noty/lib/noty.css",
              "node_modules/noty/lib/themes/relax.css",
              "node_modules/noty/lib/themes/metroui.css",
              "node_modules/@fortawesome/fontawesome-free/css/fontawesome.min.css",
              "node_modules/@fortawesome/fontawesome-free/css/brands.min.css",
              "node_modules/@fortawesome/fontawesome-free/css/solid.min.css",
              "node_modules/material-icons/iconfont/material-icons.css",
              "./app/rapydo/css/rapydo.css",
              "./app/custom/css/style.css"
            ],
            "scripts": [
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "app/rapydo/environments/environment.ts",
                  "with": "app/rapydo/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "RAPyDo:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "RAPyDo:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "RAPyDo:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "/app/app/rapydo/test.ts",
            "polyfills": "polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "./app/rapydo/assets",
              "./app/custom/assets"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/noty/lib/noty.css",
              "node_modules/noty/lib/themes/relax.css",
              "node_modules/noty/lib/themes/metroui.css",
              "node_modules/@fortawesome/fontawesome-free/css/fontawesome.min.css",
              "node_modules/@fortawesome/fontawesome-free/css/brands.min.css",
              "node_modules/@fortawesome/fontawesome-free/css/solid.min.css",
              "node_modules/material-icons/iconfont/material-icons.css",
              "./app/rapydo/css/rapydo.css",
              "./app/custom/css/style.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "RAPyDo:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "RAPyDo:serve:production"
            }
          }
        }
      }
    }},
  "defaultProject": "RAPyDo"
}
